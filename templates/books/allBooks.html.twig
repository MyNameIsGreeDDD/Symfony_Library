{% extends 'base.html.twig' %}
{% block title %} {% endblock %}
{% block body %}
    {% for book in books %}
        <a href="#">{{ book.name }}</a>
        <br>
        {{ book.author }}
        <br>
        {{ book.description }}
        <br>
        {{ book.cover }}
        <hr>
    {% endfor %}
{% endblock %}